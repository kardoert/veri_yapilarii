//INSERTÄ°ON SORT  21.03.2024
#include <iostream>
using namespace std;
void InsertionSort(int arr[], int n) 
{
    int min_index;
    for (int i = 0; i < n - 1; i++) {
        min_index = i;
        for (int j = i + 1; j < n; j++) 
        {
            if (arr[j] < arr[min_index])
            {
                min_index = j;
            }
        }
        int temp = arr[min_index];
        arr[min_index] = arr[i];
        arr[i] = temp;
    }
}
int main()
{
    int arr[] = { 12,155,78,42,96,36 };
    int n = sizeof(arr) / (sizeof (arr[0]));

    cout << "Siralanmamis hali" << endl;
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }
    cout << endl << endl;

    InsertionSort(arr, n);
    cout << "Siralanmis dizi" << endl;
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
}
